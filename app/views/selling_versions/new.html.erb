<%= form_for @selling_version do |f| %>
  <%= f.text_field :color, placeholder: "color" %>
  <%= f.text_field :condition, placeholder: "condition" %>
  <%= f.text_field :storage, placeholder: "storage" %>
  <%= f.text_field :price, placeholder: "price" %>
  <%= f.text_field :selling_store_id, placeholder: "store id" %>
  <%= f.submit %>
<% end %>
